import leafmap
import geemap

# Create an interactive map
Map = leafmap.Map()

# Load slum boundary data (replace 'slum_boundary.geojson' with your slum boundary file)
slum_boundary = geemap.geojson_to_ee('slum_boundary.geojson')

# Add slum boundary to the map
Map.addLayer(slum_boundary, {'color': 'red'}, 'Slum Boundary')

# Load road data (replace 'roads.geojson' with your road data file)
roads = geemap.geojson_to_ee('roads.geojson')

# Add roads to the map
Map.addLayer(roads, {'color': 'blue'}, 'Roads')

# Clip roads within slum boundary
roads_inside_slum = roads.filterBounds(slum_boundary)

# Add clipped roads to the map
Map.addLayer(roads_inside_slum, {'color': 'green'}, 'Roads Inside Slum')

# Display the map
Map.centerObject(slum_boundary, 12)
Map.addLayerControl()
Map
